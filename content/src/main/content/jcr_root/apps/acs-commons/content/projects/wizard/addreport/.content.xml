<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="nt:unstructured"
        jcr:title="ACS AEM Commons | Add Report"
        sling:resourceType="granite/ui/components/coral/foundation/page">
        <head jcr:primaryType="nt:unstructured">
            <viewport
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/admin/page/viewport"/>
            <favicon
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/page/favicon"/>
            <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/includeclientlibs"
                categories="[coralui3,granite.ui.coral.foundation,cq.projects.admin.startworkflow,cq.projects.admin.wizards.landingpagerequest,cq.projects.admin.form,cq.projects.admin.wizards.emailrequest,cq.common.wcm.templatepicker]"/>
        </head>
        <body
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/page/body">
            <items jcr:primaryType="nt:unstructured">
                <form
                    granite:class="cq-projects-admin-startwork-wizard"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form"
                    action="changeMe"
                    foundationForm="{Boolean}true"
                    maximized="{Boolean}true"
                    method="post"
                    novalidate="{Boolean}true"
                    style="vertical">
                    <successresponse
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/projects/admin/form/response/redirect"
                        redirectTo="${header.Referer}"/>
                    <items jcr:primaryType="nt:unstructured">
                        <operation
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                            name=":operation"
                            value="startWorkflow"/>
                        <charset
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                            name="_charset_"
                            value="utf-8"/>
                        <model
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                            name="modelId"
                            value="${param.modelId}">
                            <granite:rendercondition
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/and">
                                <exists
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/simple"
                                    expression="${not empty param.project}"/>
                            </granite:rendercondition>
                        </model>
                        <wizard
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Add Report"
                            sling:resourceType="granite/ui/components/coral/foundation/wizard"
                            cancelHref="${empty header.Referer ? granite:concat(&quot;/projects.html&quot;, granite:encodeURIPath(requestPathInfo.suffix)) : header.Referer}">
                            <items jcr:primaryType="nt:unstructured">
                                <projects
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Project"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/advancedselect"
                                    maximized="{Boolean}true"
                                    name="project"
                                    required="{Boolean}true">
                                    <granite:data
                                        jcr:primaryType="nt:unstructured"
                                        foundation-validation-ui="none"/>
                                    <field
                                        granite:class="cq-projects-startwork-projectselect"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/masonry"
                                        selectionCount="single"
                                        selectionMode="{Boolean}true">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/projects/admin/datasource/projectstiledatasource"
                                            itemResourceType="cq/gui/components/projects/admin/card/projectcard"/>
                                    </field>
                                    <parentConfig jcr:primaryType="nt:unstructured">
                                        <next
                                            granite:class="foundation-wizard-control"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/button"
                                            disabled="{Boolean}true"
                                            text="Next"
                                            variant="primary">
                                            <granite:data
                                                jcr:primaryType="nt:unstructured"
                                                foundation-wizard-control-action="next"/>
                                        </next>
                                    </parentConfig>
                                    <granite:rendercondition
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/renderconditions/or">
                                        <exists
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/simple"
                                            expression="${empty param.project}"/>
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/not">
                                            <hasPerms
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/privilege"
                                                path="${granite:concat(param.project, &quot;/jcr:content&quot;)}"
                                                privileges="[jcr:write]"/>
                                        </granite:rendercondition>
                                    </granite:rendercondition>
                                </projects>
                                <lazytemplates
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Workflow"
                                    sling:resourceType="granite/ui/components/coral/foundation/wizard/lazycontainer"
                                    src="/mnt/overlay/cq/core/content/projects/wizard/steps/projecttemplatestep.html?projectPath={project}">
                                    <parentConfig jcr:primaryType="nt:unstructured">
                                        <next
                                            granite:class="foundation-wizard-control"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/button"
                                            disabled="{Boolean}true"
                                            text="Next"
                                            variant="primary">
                                            <granite:data
                                                jcr:primaryType="nt:unstructured"
                                                foundation-wizard-control-action="next"/>
                                        </next>
                                    </parentConfig>
                                    <granite:rendercondition
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/renderconditions/or">
                                        <exists
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/simple"
                                            expression="${empty param.project}"/>
                                        <not
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                                            <hasPerms
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/privilege"
                                                path="${granite:concat(param.project, &quot;/jcr:content&quot;)}"
                                                privileges="[jcr:write]"/>
                                        </not>
                                    </granite:rendercondition>
                                </lazytemplates>
                                <templates
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Workflow"
                                    sling:resourceType="cq/gui/components/projects/admin/projectsadvancedselect"
                                    maximized="{Boolean}true"
                                    name="modelId"
                                    required="{Boolean}true">
                                    <granite:data
                                        jcr:primaryType="nt:unstructured"
                                        foundation-validation-ui="none"/>
                                    <field
                                        granite:class="cq-projects-startwork-modelselect"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/masonry"
                                        selectionCount="single"
                                        selectionMode="{Boolean}true">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/projects/admin/datasource/workflowmodelsdatasource"
                                            itemResourceType="cq/gui/components/projects/admin/card/workflowmodelcard"/>
                                    </field>
                                    <parentConfig jcr:primaryType="nt:unstructured">
                                        <next
                                            granite:class="foundation-wizard-control"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/button"
                                            disabled="{Boolean}true"
                                            text="Next"
                                            variant="primary">
                                            <granite:data
                                                jcr:primaryType="nt:unstructured"
                                                foundation-wizard-control-action="next"/>
                                        </next>
                                    </parentConfig>
                                    <granite:rendercondition
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                        <and
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <exists
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/simple"
                                                expression="${not empty param.project}"/>
                                            <hasPerms
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/privilege"
                                                path="${granite:concat(param.project, &quot;/jcr:content&quot;)}"
                                                privileges="[jcr:write]"/>
                                        </and>
                                        <or
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/or">
                                            <exists
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/simple"
                                                expression="${empty param.modelId}"/>
                                            <not
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/not">
                                                <valid
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/projects/admin/renderconditions/workflowinproject"
                                                    modelId="${param.modelId}"
                                                    projectPath="${param.project}"/>
                                            </not>
                                        </or>
                                    </granite:rendercondition>
                                </templates>
                                <lazyproperties
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Properties"
                                    sling:resourceType="granite/ui/components/coral/foundation/wizard/lazycontainer"
                                    src="\{+wizard}?project={project}&amp;modelId={modelId}">
                                    <parentConfig jcr:primaryType="nt:unstructured">
                                        <next
                                            granite:class="foundation-wizard-control"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/button"
                                            disabled="{Boolean}true"
                                            text="Submit"
                                            type="submit"
                                            variant="primary">
                                            <granite:data
                                                jcr:primaryType="nt:unstructured"
                                                foundation-wizard-control-action="next"/>
                                        </next>
                                    </parentConfig>
                                    <granite:rendercondition
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/renderconditions/or">
                                        <or
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/or">
                                            <exists
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/simple"
                                                expression="${empty param.project}"/>
                                            <not
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                                                <hasPerms
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/privilege"
                                                    path="${granite:concat(param.project, &quot;/jcr:content&quot;)}"
                                                    privileges="[jcr:write]"/>
                                            </not>
                                        </or>
                                        <or2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/or">
                                            <exists
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/simple"
                                                expression="${empty param.modelId}"/>
                                            <not
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                                                <valid
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/projects/admin/renderconditions/workflowinproject"
                                                    modelId="${param.modelId}"
                                                    projectPath="${param.project}"/>
                                            </not>
                                        </or2>
                                    </granite:rendercondition>
                                </lazyproperties>
                                <properties
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Properties"
                                    sling:resourceType="cq/gui/components/projects/admin/wfwizardinclude">
                                    <parentConfig jcr:primaryType="nt:unstructured">
                                        <next
                                            granite:class="foundation-wizard-control"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/button"
                                            disabled="{Boolean}true"
                                            text="Submit"
                                            type="submit"
                                            variant="primary">
                                            <granite:data
                                                jcr:primaryType="nt:unstructured"
                                                foundation-wizard-control-action="next"/>
                                        </next>
                                    </parentConfig>
                                    <granite:rendercondition
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                        <and
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <exists
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/simple"
                                                expression="${not empty param.project}"/>
                                            <hasPerms
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/renderconditions/privilege"
                                                path="${granite:concat(param.project, &quot;/jcr:content&quot;)}"
                                                privileges="[jcr:write]"/>
                                        </and>
                                        <and2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/renderconditions/and">
                                            <exists
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/renderconditions/simple"
                                                expression="${not empty param.modelId}"/>
                                            <valid
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/projects/admin/renderconditions/workflowinproject"
                                                modelId="${param.modelId}"
                                                projectPath="${param.project}"/>
                                        </and2>
                                    </granite:rendercondition>
                                </properties>
                            </items>
                        </wizard>
                    </items>
                </form>
            </items>
        </body>
    </jcr:content>
</jcr:root>
